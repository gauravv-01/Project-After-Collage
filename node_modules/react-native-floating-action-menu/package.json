  "scripts": {
    "start": "npm run watch",
    "build": "rimraf ./dist && babel ./src --out-dir ./dist/src && cp ./index.js ./dist/index.js && cp ./package.json ./dist/package.json",
    "watch": "rimraf ./dist && babel ./src --out-dir ./dist/src --watch && cp ./package.json ./dist/package.json",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "devtools": "react-devtools",
    "example": "cd example/ && npm install && npm start",
    "prepack": "npm run build",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },

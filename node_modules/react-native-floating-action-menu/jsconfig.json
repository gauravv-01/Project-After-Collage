{
  "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["./src/*"]
      }
  },
  "exclude": [
      "build",
      "node_modules",
      "static",
      "_pre-production",
      ".expo",
      ".expo-shared",
      ".tmp",
      ".vscode"
  ],
  "include": [
    "index.js",
    "index.d.js",
    "src/**/*"
  ]
}
